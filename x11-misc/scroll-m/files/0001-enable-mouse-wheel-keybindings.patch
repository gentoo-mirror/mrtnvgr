From fc734b27909c872178d9c86c7efbab2ac863cb1a Mon Sep 17 00:00:00 2001
From: mrtnvgr <martynovegorOF@yandex.ru>
Date: Thu, 3 Nov 2022 13:16:29 +0700
Subject: [PATCH] enable mouse wheel keybindings

---
 config.def.h | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/config.def.h b/config.def.h
index 536db70..7468b59 100644
--- a/config.def.h
+++ b/config.def.h
@@ -11,6 +11,6 @@ struct rule rules[] = {
 	{"\033[5;2~",   SCROLL_UP,   -1},	/* [Shift] + [PageUP] */
 	{"\033[6;2~",   SCROLL_DOWN, -1},	/* [Shift] + [PageDown] */
 	/* mouse binding shadows ^E and ^Y, so it's disabled by default */
-	//{"\031",        SCROLL_UP,    1},	/* mouse wheel up */
-	//{"\005",        SCROLL_DOWN,  1},	/* mouse wheel Down */
+	{"\031",        SCROLL_UP,    1},	/* mouse wheel up */
+	{"\005",        SCROLL_DOWN,  1},	/* mouse wheel Down */
 };
